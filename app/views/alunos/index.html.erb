<p id="notice"><%= notice %></p>

<h1>Lista Alunos</h1>

<table style="width:100%">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>Cpf</th>
      <th>Endereco</th>
      <th>Profissao</th>
      <th>Email</th>
      <th>Cnh</th>
      <th>Escolaridade</th>
      <th>Instituicao</th>
      <th>Curso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.nome %></td>
        <td><%= aluno.sobrenome %></td>
        <td><%= aluno.cpf %></td>
        <td><%= aluno.endereco %></td>
        <td><%= aluno.profissao %></td>
        <td><%= aluno.email %></td>
        <td><%= aluno.cnh %></td>
        <td><%= aluno.escolaridade %></td>
        <td><%= aluno.instituicao %></td>
        <td><%= aluno.curso %></td>
        <td><%= link_to 'Show', aluno %></td>
        <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
        <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aluno', new_aluno_path %>
